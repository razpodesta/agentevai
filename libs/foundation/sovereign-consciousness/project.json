{
  "name": "sovereign-consciousness",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/foundation/sovereign-consciousness/src",
  "projectType": "library",
  "metadata": {
    "description": "Motor de Consciência Estática: Isola rastro territorial e cultural imutável para execução em Edge Runtime.",
    "author": "Raz Podestá - MetaShark Tech"
  },
  "tags": [
    "type:util",
    "scope:foundation",
    "tier:infrastructure",
    "runtime:edge-ready"
  ],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/foundation/sovereign-consciousness",
        "main": "libs/foundation/sovereign-consciousness/src/index.ts",
        "tsConfig": "libs/foundation/sovereign-consciousness/tsconfig.lib.json",
        "assets": ["libs/foundation/sovereign-consciousness/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/foundation/sovereign-consciousness/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
