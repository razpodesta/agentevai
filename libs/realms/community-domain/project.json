{
  "name": "community-domain",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/realms/community-domain/src",
  "projectType": "library",
  "tags": ["type:domain", "scope:community", "role:social-logic"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/realms/community-domain",
        "main": "libs/realms/community-domain/src/index.ts",
        "tsConfig": "libs/realms/community-domain/tsconfig.lib.json",
        "assets": ["libs/realms/community-domain/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": { "jestConfig": "libs/realms/community-domain/jest.config.ts" }
    }
  }
}