{
  "name": "geography-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/realms/geography-ui/src",
  "projectType": "library",
  "metadata": {
    "description": "Interface Cartográfica Cinética: Renderiza a malha hexagonal H3 e mapas de calor de entropia social.",
    "author": "Raz Podestá - MetaShark Tech"
  },
  "tags": [
    "type:ui",
    "scope:realms",
    "role:h3-visualization",
    "tier:presentation"
  ],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/realms/geography-ui",
        "configFile": "libs/realms/geography-ui/vite.config.mts"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "reportsDirectory": "../../../coverage/libs/realms/geography-ui"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
